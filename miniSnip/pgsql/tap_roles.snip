? pg_tap roles
begin;
select plan(1);

select roles_are(array[
  '<{owner}>',
  'anon',
  'authenticator',
  'pg_execute_server_program',
  'pg_monitor',
  'pg_read_all_settings',
  'pg_read_all_stats',
  'pg_read_server_files',
  'pg_signal_backend',
  'pg_stat_scan_tables',
  'pg_write_server_files',
  'superuser'
]);

select * from finish();
rollback;
